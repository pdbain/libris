package Libris;

public class LibrisDatabase {
	private BloomFilter keywordFilter = new BloomFilter();
	private DatabaseReader reader;
	protected void open(){
		
	}
	LibrisRecord foo;
	int bar;
	protected void addRecord(LibrisRecord record){
		foo.setRecNum(bar);
		int recNum = record.getRecNum();
		keywordFilter.addKeywords(recNum, record.getKeywords());
	}
}
